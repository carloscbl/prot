{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/build/api-server",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Rest_Server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/api-server",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/prot",
            //"args": ["test","sequence", "-p", "../sequences/request"],
            "args": ["test","sequence", "-p", "../sequences/request_task_movement"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) test gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test_prot",
            //"args": ["test","sequence", "-p", "../sequences/request"],
            "args": ["[projector]"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [
                {"name":"PROT_DB_PASSW", "value":"Prot#654321"},
                {"name":"PROT_DB_DEBUG", "value":"true"}
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "compile_test"
        },
        {
            "name": "(gdb) test clang++",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test_prot",
            //"args": ["test","sequence", "-p", "../sequences/request"],
            //"args": ["user","sch", "-s", "90", "-e" , "99"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [
                {"name":"PROT_DB_PASSW", "value":"Prot#654321"}
            ],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "compile_test"
        }
    ]
}