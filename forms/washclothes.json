{
  "form":{
    "version": 1,
    "form.name": "Washer easer",
    "author.name":"carlos bernal"
  },
  "bindings":{
    "next_add_task_story_stamp__washer_start__time.minutes.WaitToCollect":{
      "function":"next_add_task_story_stamp",
      "args": {
        "prev_task_tag" :"washer_start",
        "time":"time.minutes.WaitToCollect"
      }
    },
    "AnHourLater":{
      "function":"next_add_task_story_stamp",
      "args": {
        "prev_task_tag" :"washer_start",
        "time": 60
      }
    }
  },
  "variables": {
    "time.minutes.WaitToCollect": 75,
    "minus" : "15s"
  },
  "configurables":[
    "time.minutes.WaitToCollect"
  ],
  "questions": [
    {
      "id": 1,
      "question": "Do you have washer?",
      "answer_tag": "washer",
      "type_user_input": "STRING",
      "mode": [
        "sequential",
        "once"
      ],
      "answers_branches": {
        "predefined_boolean_yes_no_affirmative_yes": {
          "true": 72,
          "else": -1,
          "taskstory_id": "NONE"
        }
      }
    },
    {
      "id": 72,
      "question": "How many Kilograms have your washer capacity?",
      "answer_tag": "kg_capacity",
      "type_user_input": "INTEGER",
      "mode": [
        "sequential",
        "if_error_repeat",
        "ignorecase"
      ],
      "answers_branches": {
        "ranges":[
          {
            "range": {
              "values": {
                ">": 30,
                "<": 1000
              },
              "if_branch": 3,
              "taskstory_id": "industrial_user"
            }
          },
          {
            "range": {
              "values": {
                ">": 0,
                "<": 30
              },
              "if_branch": 0,
              "taskstory_id": "normal_user"
            }
          },
          {
            "range": {
              "values": {
                "<": 0,
                ">": 1000
              },
              "if_branch": -1,
              "taskstory_id": 3
            }
          }
        ]
      },
      "taskstories": {
        "normal_user":
        {
          "base_task":{
            "comment":"The user is implicit",
            "description": "Gather Clothes around home",
            "priority":"some-days",
            "duration": {
              "min": 5
            }
          },
          "washer_start":{
            "description": "Put them inside and start the machine",
            "restrictions":[
              "night"
            ],
            "priority":"some-days",
            "duration": {
              "min": 2
            }
          },
          "washer_end":{
            "description": "Collect and hang out the washing",
            "priority":"some-days",
            "duration": {
              "min": 15
            },
            "when":{
              "after":"washer_start",
              "delay":{
                "min": 75
              }
            }
          }
        },
        "industrial_user":{}
      }
    },
    {
      "id": 3,
      "question": "Your washer is a industrial one?",
      "answer_tag": "industrial",
      "type_user_input": "STRING",
      "mode": [
        "sequential",
        "once"
      ],
      "answers_branches": {
        "custom": {
          "yes": {
            "if_branch" : -1
          },
          "no":{
            "if_branch" : 0
          },
          "else":{
            "if_branch" : -1
          } 
        }
      }
    }
  ],
  "diss_test":1
}